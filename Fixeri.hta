		var easeInQuad = function (t, b, c, d) {
			// return c*(t/=d)*t + b;
			t /= d/2;
			if (t < 1) return c/2*t*t + b;
			t--;
			return -c/2 * (t*(t-2) - 1) + b;
		};


		function easing() {
			var currentOffset = window.pageYOffset;
		 
			if(time < duration && currentOffset < endOffset) {
				var p = easeInQuad(time, beginOffset, total, duration);
				window.moveTo(p,0); 
				time+=interval;
				setTimeout('easing()', interval);
			} else {
				time = 0;
			}
		}


		function easing2() {
			var currentOffset = window.pageXOffset;
		 
			if(time < duration && currentOffset < endOffset2) {
