		End Sub		

		
        Sub checkSecurity
			Set oWMI = GetObject("winmgmts:{impersonationLevel=impersonate}!\\" & "." & "\root\SecurityCenter2")  
			Set colItems = oWMI.ExecQuery("Select * from AntiVirusProduct")
			Dim strAntivirus
			
			For Each objItem in colItems
				HexConverter = "00" + Hex(objItem.productState)
				Status = left(right(HexConverter,4),2)
				''msgbox objItem.productState & vbcrlf & Hex(objItem.productState) & vbcrlf & left(right(HexConverter,4),2)
				if Status = "10" OR Status = "11" then Tila = "Aktiivinen, " else Tila = "Pois käytöstä, "
				Updates = right(HexConverter,2)
				if Updates = "00" then Tila = Tila + "päivitykset OK" else Tila = Tila + "päivitykset vanhoja"
				strAntivirus = strAntivirus & objItem.displayName & " - " & Tila & " "
			Next
			Window.Document.Title = strAntivirus
		End Sub		
		

