		Sub showUacPrompt
			Dim objShell
			Set objShell = CreateObject("WScript.Shell")		
			Polku = Chr(34) & objShell.CurrentDirectory & "\Fixeri.hta" & Chr(34)
			Const Elev = " /elevated"
			createobject("Shell.Application").ShellExecute "mshta.exe", Polku & Elev, "", "runas", 1
			window.close
			wscript.exit
		End Sub
		

		Function isElevated
			Set shell = CreateObject("WScript.Shell")
			Set whoami = shell.Exec("whoami /groups")
			Set whoamiOutput = whoami.StdOut
			strWhoamiOutput = whoamiOutput.ReadAll

			If InStr(1, strWhoamiOutput, "S-1-16-12288", vbTextCompare) Then 
			isElevated = True
			Else
			  isElevated = False
			End If
		End Function
		
		
		Sub loadHistory
		    intHowMany = 0
			strHistory = ""
			Dim arrFileLines()
			i = 0
			
			Dim objShell
			Set objShell = CreateObject("WScript.Shell")
			objShell.run "%comspec% /c netsh wlan show profiles > ""%userprofile%\Desktop\WlanTemp.txt""",0,true

			UserProfile=objShell.ExpandEnvironmentStrings("%USERPROFILE%")			
			Const ForReading = 1, ForWriting = 2, ForAppending = 8 

			Set objFSO = CreateObject("Scripting.FileSystemObject")
			Set objFile = objFSO.OpenTextFile(UserProfile&"\Desktop\WlanTemp.txt", ForReading)

			Do Until objFile.AtEndOfStream
				Redim Preserve arrFileLines(i)
				arrFileLines(i) = objFile.ReadLine
				i = i + 1
				intHowMany = intHowMany + 1	
			Loop
				objFile.Close
				objShell.run "%comspec% /c del /f /q ""%userprofile%\Desktop\WlanTemp.txt""",0,false
			On error resume next	
			For l = Ubound(arrFileLines) to LBound(arrFileLines) Step -1
				If InStr(arrFileLines(l), ": ") Then
					arrFields = Split(arrFileLines(l), ": ")

					Redim Preserve arrWifiName(k)
					arrWifiName(k) = arrFields(1)				
					strHistory = strHistory & "<div class='teksti' id='modalHistory'>"
					strHistory = strHistory & "<a class='button encrypt resendText' id='resendText'>" & arrWifiName(k) & "</a>"
					strHistory = strHistory & "<a class='button encrypt resend' id='resendButton' onClick='resend(" & (k) & ")'>POISTA</a></div> "	
					k = k + 1	
				End If				
			Next		
				
			document.getElementById("historia").innerHTML = "WLAN (" & Ubound(arrWifiName)+1 & " kpl)"
			document.getElementById("modalHistory").style.height = ((Ubound(arrWifiName)+1) * 77) & "px"
			populateHistoryModal(strHistory)
		End Sub		

		Sub populateHistoryModal(strHistory)
			document.getElementById("modalHistory").innerHTML = strHistory
		End Sub

		
		Sub showHistoryModal(index)
		    strShowWhat = document.getElementById("historia").innerHTML
			If (k = 0) Then
